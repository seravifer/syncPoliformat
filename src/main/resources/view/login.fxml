<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="255.0" prefWidth="270.0" xmlns="http://javafx.com/javafx/8.0.131" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <TextField fx:id="usernameID" focusTraversable="false" layoutX="61.0" layoutY="71.0" promptText="DNI" />
      <PasswordField fx:id="passwordID" focusTraversable="false" layoutX="61.0" layoutY="111.0" promptText="Contraseña" />
      <Button fx:id="loginID" focusTraversable="false" layoutX="125.0" layoutY="182.0" mnemonicParsing="false" onAction="#loginClick" text="Iniciar sesión">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <CheckBox fx:id="rememberID" focusTraversable="false" layoutX="63.0" layoutY="146.0" mnemonicParsing="false" text="Recordar mi cuenta" />
   </children>
</AnchorPane>
