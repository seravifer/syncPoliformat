<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="255.0" prefWidth="270.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <TextField fx:id="usernameID" layoutX="52.0" layoutY="95.0" prefHeight="25.0" prefWidth="167.0" promptText="DNI" />
      <PasswordField fx:id="passwordID" layoutX="52.0" layoutY="135.0" prefHeight="25.0" prefWidth="167.0" promptText="Contraseña" />
      <CheckBox fx:id="rememberID" layoutX="52.0" layoutY="169.0" mnemonicParsing="false" text="Recordarme" />
      <Button fx:id="loginID" layoutX="135.0" layoutY="202.0" mnemonicParsing="false" onAction="#loginClick" text="Iniciar sesión">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <ImageView cache="true" fitHeight="87.0" fitWidth="199.0" layoutX="36.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../res/logo-poliformat.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
